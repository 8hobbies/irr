{
  "name": "@hong-xu/irr",
  "version": "1.0.0",
  "description": "A package for computing the internal rate of return.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "/dist",
    "!/dist/**.test.d.ts",
    "!/dist/**.test.js",
    "!/dist/jest.config.d.ts",
    "!/dist/jest.config.js"
  ],
  "scripts": {
    "clean": "tsc --build --clean",
    "compile": "tsc",
    "doc": "typedoc",
    "format": "eslint --fix --ext .ts . && prettier . --write",
    "lint": "eslint --ext .ts . && prettier . --check",
    "prepack": "npm run clean && npm run compile",
    "test": "npm run compile && jest",
    "test-all": "npm run compile && npm run lint && npm run test && npm run doc"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/xuhdev/irr.git"
  },
  "keywords": [
    "finance"
  ],
  "author": "Hong Xu",
  "license": " Apache-2.0",
  "bugs": {
    "url": "https://gitlab.com/xuhdev/irr/issues"
  },
  "homepage": "https://irr.topbug.net",
  "devDependencies": {
    "@babel/preset-env": "^7.23.9",
    "@babel/preset-typescript": "^7.23.3",
    "@jest/globals": "^29.7.0",
    "@types/node": "^20.11.17",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "eslint": "^8.56.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2",
    "typedoc": "^0.25.8",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "ml-matrix": "^6.11.0"
  }
}
